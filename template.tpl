___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const url = 'https://cdn.pdst.fm/ping.min.js';

//Uncomment to debug
//log('PodSights GTM Settings', data);

// Capture user input from the tag
const pixelId = data.pixelId;
const eventName = data.eventName;

// If the script loaded successfully, log a message and signal success
const onSuccess = () => {
  callInWindow('pdst', 'conf', {'key': pixelId});
  callInWindow('pdst', eventName);
  //Uncomment to debug
  //log('PodSights script loaded', eventName, pixelId);
  data.gtmOnSuccess();
};

// If the script fails to load, log a message and signal failure
const onFailure = () => {
  log('PodSights script failed');
  data.gtmOnFailure();
};

// If the URL input by the user matches the permissions set for the template,
// inject the script with the onSuccess and onFailure methods as callbacks.
if (queryPermission('inject_script', url)) {
  injectScript(url, onSuccess, onFailure);
} else {
  log('PodSights: Script load failed due to permissions mismatch.');
  data.gtmOnFailure();
}

// Load the PodSight script if not already loaded
injectScript(url, onSuccess, onFailure, '_pdst_script');


